{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "restfulapis": {
      "root": "apps/restfulapis",
      "sourceRoot": "apps/restfulapis/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/restfulapis",
            "main": "apps/restfulapis/src/main.ts",
            "tsConfig": "apps/restfulapis/tsconfig.app.json",
            "assets": ["apps/restfulapis/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/restfulapis/src/environments/environment.ts",
                  "with": "apps/restfulapis/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "restfulapis:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/restfulapis/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/restfulapis"],
          "options": {
            "jestConfig": "apps/restfulapis/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "taskmanager": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/taskmanager",
      "sourceRoot": "apps/taskmanager/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/taskmanager",
            "index": "apps/taskmanager/src/index.html",
            "main": "apps/taskmanager/src/main.ts",
            "polyfills": "apps/taskmanager/src/polyfills.ts",
            "tsConfig": "apps/taskmanager/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/taskmanager/src/favicon.ico",
              "apps/taskmanager/src/assets"
            ],
            "styles": ["apps/taskmanager/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/taskmanager/src/environments/environment.ts",
                  "with": "apps/taskmanager/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "taskmanager:build:production"
            },
            "development": {
              "browserTarget": "taskmanager:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/taskmanager/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "taskmanager:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/taskmanager/src/test.ts",
            "polyfills": "apps/taskmanager/src/polyfills.ts",
            "tsConfig": "apps/taskmanager/tsconfig.spec.json",
            "karmaConfig": "apps/taskmanager/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/taskmanager/src/favicon.ico",
              "apps/taskmanager/src/assets"
            ],
            "styles": ["apps/taskmanager/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "taskmanager",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  }
}
